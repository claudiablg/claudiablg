/**
 * For typescript libraries for the browser, meaning that those output files will
 * be handled by webpack & loaders
 *
 * @example ui component libraries, utilities for applications, etc.
 */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "declaration": true,
    "incremental": true,
    "composite": true,
    "lib": ["dom", "ESNext"]
  }
}
