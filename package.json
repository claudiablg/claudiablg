{
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "14"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "autorelease": ". ./.env && lerna version --conventional-commits --create-release github",
    "build": "lerna run build",
    "build:core": "yarn build --scope @newrade/core-*",
    "build:core:lib": "yarn build --scope @newrade/core-* --ignore @newrade/core-gatsby-starter --ignore @newrade/core-react-ui-app",
    "build:mir": "yarn build --scope @newrade/mir-*",
    "build:newrade": "yarn build --scope @newrade/newrade-* --ignore @newrade/newrade-app",
    "build:starter": "yarn build --scope @newrade/core-gatsby-starter",
    "build:valentine": "yarn build --scope @newrade/mir-*",
    "build:vsb": "yarn build --scope @newrade/vsb-*",
    "build:website": "yarn build --scope @newrade/*-website --scope @newrade/core-gatsby-starter",
    "changelog": "lerna version --conventional-commits --no-push",
    "clean": "lerna run clean",
    "cli": "nr",
    "commit": "lint-staged && git-cz",
    "fork": "ts-node --script-mode scripts/fork-repo.ts",
    "format:changelog": "prettier --write packages/**/CHANGELOG.md --loglevel silent && git add packages/**/CHANGELOG.md",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "nvm": "ts-node --script-mode scripts/install-node-nvm.ts",
    "postinstall": "yarn clean && yarn build:core:lib",
    "push": "git push --follow-tags",
    "reset": "npm i -g rimraf && rimraf -rf ./node_modules packages/**/node_modules && yarn",
    "set-permissions": "chmod a+x node_modules/.bin/nr-core-cli",
    "start": "lerna run --parallel start",
    "start:core": "yarn start --scope @newrade/core-*",
    "start:core:lib": "yarn start --scope @newrade/core-* --ignore @newrade/core-gatsby-starter --ignore @newrade/core-react-ui-app",
    "start:mir": "yarn start --scope @newrade/mir-*",
    "start:newrade": "yarn start --scope @newrade/newrade-* --ignore @newrade/newrade-app",
    "start:starter": "yarn start --scope @newrade/core-gatsby-starter",
    "start:valentine": "yarn build --scope @newrade/mir-*",
    "start:vsb": "yarn build --scope @newrade/vsb-*",
    "start:website": "yarn start --scope @newrade/*-website --scope @newrade/core-gatsby-starter",
    "test": "lerna run --concurrency=1 test",
    "test:core:ui": "yarn test --scope '{@newrade/core-design-system,@newrade/core-react-ui,@newrade/core-react-ui-app}'",
    "yo": "nr-core-yo-generator"
  },
  "resolutions": {
    "webpack": "^4.43.0"
  },
