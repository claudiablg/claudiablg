{
  "name": "@newrade/core-design-system-docs",
  "version": "2.3.0",
  "description": "Markdown based documentation for design systems",
  "license": "MIT",
  "files": [
    "/src"
  ],
  "scripts": {
    "build": "yarn clean && yarn typecheck",
    "clean": "yarn clean:tsc && yarn clean:dist",
    "clean:dist": "rimraf -rf dist/** lib/**",
    "clean:tsc": "rimraf -f ./**/*.tsbuildinfo *.tsbuildinfo",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": [
    "*.css",
    "*.treat.ts",
    "*.treat.js"
  ],
  "dependencies": {
    "@newrade/core-common": "^1.4.2",
    "@newrade/core-design-system": "^1.14.2",
    "@newrade/core-gatsb-config": "^3.1.0",
    "@newrade/core-gatsby-ui": "^2.3.0",
    "@newrade/core-react-ui": "^2.3.0",
    "@newrade/core-types": "*",
    "@newrade/core-utils": "^2.3.0",
    "@newrade/core-webpack-config": "^2.2.0",
    "@newrade/core-website-api": "^1.4.0",
    "gatsby": "^3.14.2",
    "gatsby-plugin-image": "^1.14.1",
    "react-icons": "^4.1.0"
  }
}
