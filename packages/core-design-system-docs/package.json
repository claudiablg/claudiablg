{
  "name": "@newrade/core-design-system-docs",
  "version": "2.1.0",
  "description": "Markdown based documentation for design systems",
  "license": "MIT",
  "files": [
    "/src"
  ],
  "scripts": {
    "build": "yarn clean && yarn typecheck",
    "clean": "yarn clean:tsc && yarn clean:dist",
    "clean:dist": "rm -rf dist/** lib/**",
    "clean:tsc": "rm -f ./**/*.tsbuildinfo *.tsbuildinfo",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": [
    "*.css",
    "*.treat.ts",
    "*.treat.js"
  ],
  "dependencies": {
    "@newrade/core-design-system": "^1",
    "@newrade/core-gatsby-ui": "^2",
    "@newrade/core-gsap-ui": "^1.2.0",
    "@newrade/core-react-ui": "^2",
    "gatsby": "^3.10.2",
    "gatsby-plugin-image": "^1.11.0-next.2"
  },
  "devDependencies": {
    "@newrade/core-common": "^1",
    "@newrade/core-gatsb-config": "^2",
    "@newrade/core-types": "^2",
    "@newrade/core-utils": "^2",
    "@newrade/core-webpack-config": "^2",
    "@newrade/core-website-api": "^1.1.0"
  }
}
