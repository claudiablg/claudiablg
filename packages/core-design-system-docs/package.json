{
  "name": "@newrade/core-design-system-docs",
  "version": "2.1.0",
  "description": "Markdown based documentation for design systems",
  "license": "MIT",
  "main": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build-disabled": "tsc --build",
    "clean": "rm -rf lib/** && rm -f *.tsbuildinfo",
    "start-disabled": "yarn typecheck",
    "test-disabled": "jest",
    "test:watch-disabled": "jest --watch",
    "typecheck": "yarn typecheck:build --watch --preserveWatchOutput --verbose",
    "typecheck:build": "tsc --project tsconfig.build.json --noEmit"
  },
  "sideEffects": false,
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@newrade/core-common": "^1",
    "@newrade/core-gatsby-config": "^2",
    "@newrade/core-gatsby-ui": "^2",
    "@newrade/core-react-ui": "^2",
    "@newrade/core-types": "^2",
    "@newrade/core-utils": "^2",
    "@newrade/core-webpack-config": "^2",
    "@newrade/core-website-api": "^1"
  }
}
