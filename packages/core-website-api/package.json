{
  "name": "@newrade/core-website-api",
  "version": "1.2.2",
  "description": "A collection of models to represent data in websites.",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rm -rf lib/** && rm -f tsconfig.build.tsbuildinfo",
    "start": "tsc --build tsconfig.build.json --watch --preserveWatchOutput",
    "typecheck": "yarn typecheck:build --watch --preserveWatchOutput",
    "typecheck:build": "tsc --project tsconfig.build.json --noEmit"
  },
  "sideEffects": false,
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@newrade/core-common": "^1.4.0",
    "@newrade/core-design-system": "^1.14.0",
    "@newrade/core-types": "^2.0.0",
    "@newrade/core-utils": "^2.0.0"
  }
}
