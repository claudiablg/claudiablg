{
  "github": {
    "enabled": false
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "www.vasectomie-pierre-boucher.ca/api/(.*)",
      "destination": "https://api.vasectomie-pierre-boucher.ca/api"
    },
    {
      "source": "vasectomie-pierre-boucher.ca/api/(.*)",
      "destination": "https://api.vasectomie-pierre-boucher.ca/api"
    },
    {
      "source": "(pr-\\d+|dev|master).vasectomie-pierre-boucher.ca/api/(.*)",
      "destination": "https://$1-api.vasectomie-pierre-boucher.ca/api/$2"
    }
  ],
  "redirects": [
    { "source": "/boucher.php", "destination": "/equipe/", "permanent": true },
    { "source": "/boucher", "destination": "/equipe/", "permanent": true },
    { "source": "/plan.php", "destination": "/", "permanent": true },
    { "source": "/plan", "destination": "/", "permanent": true },
    { "source": "/consultation.php", "destination": "/vasectomie/", "permanent": true },
    { "source": "/consultation", "destination": "/vasectomie/", "permanent": true },
    { "source": "/vasectomie.php", "destination": "/vasectomie/", "permanent": true },
    { "source": "/vasectomie", "destination": "/vasectomie/", "permanent": true },
    { "source": "/couts.php", "destination": "/vasectomie/", "permanent": true },
    { "source": "/couts", "destination": "/vasectomie/", "permanent": true },
    { "source": "/spermogramme.php", "destination": "/vasectomie/", "permanent": true },
    { "source": "/spermogramme", "destination": "/vasectomie/", "permanent": true },
    { "source": "/etapes.php", "destination": "/vasectomie/", "permanent": true },
    { "source": "/etapes", "destination": "/vasectomie/", "permanent": true },
    { "source": "/meac.php", "destination": "/examen-pour-transports-canada/", "permanent": true },
    { "source": "/meac", "destination": "/examen-pour-transports-canada/", "permanent": true },
    { "source": "/media/pdf/depliant-preparatoire-vasectomie.pdf", "destination": "/vasectomie/", "permanent": true },
    { "source": "/media/pdf/carton-informations-vasectomie.pdf", "destination": "/vasectomie/", "permanent": true },
    { "source": "/joindre.php", "destination": "/contact/", "permanent": true },
    { "source": "/joindre", "destination": "/contact/", "permanent": true }
  ]
}
