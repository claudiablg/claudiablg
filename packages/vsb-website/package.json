{
  "name": "@newrade/vsb-website",
  "version": "1.10.0",
  "description": "Gatsby website for vsb a medical website",
  "author": {
    "email": "info@newrade.ca",
    "name": "VSB",
    "url": "https://github.com/newrade/newrade"
  },
  "repository": "https://github.com/newrade/newrade/tree/master/packages/newrade-website",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn typecheck:build",
    "build": "gatsby build",
    "build:local": ". ./.env && DEBUG=$DEBUG gatsby build",
    "clean": "gatsby clean",
    "compile": "tsc --project tsconfig.gatsby.json",
    "contentful-types": ". ./.env && contentful-tools generate --config=contentful-tools.js --token=$CONTENTFUL_MANAGEMENT_TOKEN_VSB",
    "copy:functions": "TS_NODE_PROJECT=tsconfig.script.json node -r ts-node/register scripts/copy-functions.script.ts",
    "pre-debug": "yarn clean",
    "preload-fonts": ". ./.env && PORT=$APP_PORT gatsby-preload-fonts --no-sandbox",
    "refresh": ". ./.env && curl -X POST http://localhost:$APP_PORT/__refresh",
    "serve": ". ./.env && NODE_ENV=production gatsby serve --port=$APP_PORT",
    "start": ". ./.env && DEBUG=$DEBUG gatsby develop -p $APP_PORT -H 0.0.0.0",
    "test": "echo \"Wrtps://gatsby.dev/unit-testing\"",
    "test:e2e": ". ./.env && DEBUG=$DEBUG jest --config jest-e2e.config.js",
    "typecheck": "yarn typecheck:build --watch --preserveWatchOutput",
    "typecheck:build": "tsc --project tsconfig.build.json --noEmit"
  },
  "dependencies": {
    "@newrade/core-design-system": "^1.9.0",
    "@newrade/core-gsap-ui": "^1.1.3",
    "@newrade/core-react-ui": "^1.11.0",
    "@newrade/vsb-api": "^1.3.1",
    "@newrade/vsb-common": "^1.3.1",
    "gatsby": "^2.29"
  },
  "devDependencies": {
    "@newrade/core-common": "^1.2.1",
    "@newrade/core-gatsby-config": "^1.9.0",
    "@newrade/core-gatsby-ui": "^1.5.0",
    "@newrade/core-types": "^1.3.1",
    "@newrade/core-utils": "^1.6.1",
    "@newrade/core-webpack-config": "^1.4.3"
  }
}
