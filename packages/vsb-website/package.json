{
  "name": "@newrade/vsb-website",
  "version": "2.1.0",
  "description": "VSB main website",
  "author": {
    "email": "info@newrade.ca",
    "name": "VSB",
    "url": "https://github.com/newrade/newrade"
  },
  "repository": "https://github.com/newrade/newrade/tree/master/packages/newrade-website",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:local": ". ./.env && DEBUG=$DEBUG gatsby build",
    "clean": "gatsby clean",
    "compile": "tsc --project tsconfig.gatsby.json",
    "contentful-types": ". ./.env && contentful-tools generate --config=contentful-tools.js --token=$CONTENTFUL_MANAGEMENT_TOKEN_VSB",
    "pre-debug": "yarn clean",
    "preload-fonts": ". ./.env && PORT=$APP_PORT gatsby-preload-fonts --no-sandbox",
    "refresh": ". ./.env && curl -X POST http://localhost:$APP_PORT/__refresh",
    "serve": ". ./.env && NODE_ENV=production gatsby serve --port=$APP_PORT --host 0.0.0.0",
    "serve:ci:start": ". ./.env && NODE_ENV=production forever start \"node_modules/.bin/gatsby\" serve --port=$APP_PORT --host 0.0.0.0",
    "serve:ci:stop": "forever stopall",
    "start": ". ./.env && DEBUG=$DEBUG gatsby develop -p $APP_PORT -H 0.0.0.0",
    "test": "echo ok",
    "test:e2e": "jest --config jest-e2e.config.js",
    "typecheck": "yarn typecheck:build --watch --preserveWatchOutput",
    "typecheck:build": "tsc --project tsconfig.build.json --noEmit",
    "vercel-deploy": ". ./.env && VERCEL_PROJECT_ID=$VERCEL_PROJECT_ID VERCEL_ORG_ID=$VERCEL_ORG_ID yarn vercel public --token $VERCEL_TOKEN --scope $VERCEL_SCOPE --confirm"
  },
  "eslintConfig": {},
  "dependencies": {
    "@auth0/auth0-react": "^1.4.0",
    "@newrade/core-design-system": "^1.10.1",
    "@newrade/core-gsap-ui": "^1.1.7",
    "@newrade/core-react-ui": "^1.14.0",
    "@newrade/vsb-api": "^1.4.1",
    "@newrade/vsb-common": "^1.4.1",
    "gatsby": "^2.32.13",
    "react-final-table": "^1.5.1"
  },
  "devDependencies": {
    "@newrade/core-common": "^1.3.0",
    "@newrade/core-design-system-docs": "^1.4.0",
    "@newrade/core-gatsby-config": "^1.12.0",
    "@newrade/core-gatsby-ui": "^1.8.0",
    "@newrade/core-types": "^1.5.0",
    "@newrade/core-utils": "^1.7.0",
    "@newrade/core-webpack-config": "^1.6.1"
  }
}
