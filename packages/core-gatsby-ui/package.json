{
  "name": "@newrade/core-gatsby-ui",
  "version": "1.3.0",
  "description": "Reusable UI components for Gatsby sites",
  "license": "MIT",
  "main": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build-disabled": "tsc --build tsconfig.build.json",
    "clean": "rimraf -r lib/** && rm -f tsconfig.build.tsbuildinfo",
    "start": "yarn typecheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "yarn typecheck:build --watch --preserveWatchOutput",
    "typecheck:build": "tsc --project tsconfig.build.json --noEmit"
  },
  "sideEffects": false,
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@newrade/core-common": "^1.1.0",
    "@newrade/core-gatsby-config": "^1.7.0",
    "@newrade/core-react-ui": "^1.8.0",
    "@newrade/core-types": "^1.1.0",
    "@newrade/core-utils": "^1.5.0",
    "@newrade/core-webpack-config": "^1.4.0"
  }
}
