{
  "name": "@newrade/core-gatsby-ui",
  "version": "2.2.1",
  "description": "Reusable UI components for Gatsby sites",
  "license": "MIT",
  "main": "",
  "files": [
    "/src"
  ],
  "scripts": {
    "build": "yarn clean && yarn typecheck",
    "clean": "yarn clean:tsc && yarn clean:dist",
    "clean:dist": "rm -rf dist/** lib/**",
    "clean:tsc": "rm -f ./**/*.tsbuildinfo *.tsbuildinfo",
    "test": "nr jest",
    "test:watch": "nr jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": [
    "*.css",
    "*.treat.ts",
    "*.treat.js"
  ],
  "dependencies": {
    "@mdx-js/react": "^1.6.22",
    "@newrade/core-common": "*",
    "@newrade/core-design-system": "*",
    "@newrade/core-gatsb-config": "^3.0.1",
    "@newrade/core-jest-config": "*",
    "@newrade/core-react-ui": "^2.2.1",
    "@newrade/core-types": "*",
    "@newrade/core-website-api": "^1.3.1",
    "@reach/router": "^1",
    "@react-google-maps/api": "^2.1.1",
    "@vanilla-extract/css": "^1.1.0",
    "capsize": "^1",
    "case": "^1.6.3",
    "gatsby": "^3.13.0",
    "gatsby-background-image": "^1",
    "gatsby-image": "^2.9.0",
    "i18next": "^19",
    "react-i18next": "^11",
    "react-icons": "^4.1.0",
    "react-intersection-observer": "^8.31.0",
    "react-treat": "^2",
    "treat": "^2"
  }
}
