{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Gatsby develop",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "program": "${workspaceFolder}/node_modules/.bin/gatsby",
      "args": ["develop", "--inspect-brk"],
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "gatsby:*"
      },
      "stopOnEntry": false,
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "internalConsoleOptions": "neverOpen",
      "sourceMaps": true,
      "preLaunchTask": "clean"
    },
    {
      "name": "Gatsby build",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "program": "${workspaceFolder}/../../node_modules/.bin/gatsby",
      "args": ["build"],
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "gatsby:*"
      },
      "stopOnEntry": false,
      "runtimeArgs": ["--nolazy"],
      "sourceMaps": false
    }
  ]
}
