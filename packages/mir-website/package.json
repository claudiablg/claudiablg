{
  "name": "@newrade/mir-website",
  "version": "1.7.1",
  "description": "Gatsby website for newrade's main website",
  "author": {
    "email": "info@newrade.ca",
    "name": "Newrade",
    "url": "https://github.com/newrade/newrade"
  },
  "repository": "https://github.com/newrade/newrade/tree/master/packages/newrade-website",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn compile",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "compile": "tsc --project tsconfig.gatsby.json",
    "contentful-types": ". ./.env && contentful-tools generate --config=contentful-tools.js --token=$CONTENTFUL_MANAGEMENT_TOKEN_MIR",
    "pre-debug": "yarn clean && yarn compile",
    "preload-fonts": ". ./.env && PORT=$APP_PORT gatsby-preload-fonts --no-sandbox",
    "refresh": ". ./.env && curl -X POST http://localhost:$APP_PORT/__refresh",
    "serve": "gatsby serve",
    "prestart": "yarn clean && yarn compile",
    "start": ". ./.env && gatsby develop -p $APP_PORT -H 0.0.0.0",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@newrade/core-design-system-old": "^1.3.3",
    "@newrade/core-react-ui-old": "^1.5.2",
    "gatsby": "^2.24"
  },
  "devDependencies": {
    "@newrade/core-gatsby-config": "^1.4.2",
    "@newrade/core-webpack-config": "^1.3.2"
  }
}
