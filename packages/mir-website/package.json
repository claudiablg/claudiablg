{
  "name": "@newrade/mir-website",
  "version": "1.5.0",
  "description": "Gatsby website for newrade's main website",
  "author": {
    "email": "info@newrade.ca",
    "name": "Newrade",
    "url": "https://github.com/newrade/newrade"
  },
  "repository": "https://github.com/newrade/newrade/tree/master/packages/newrade-website",
  "license": "MIT",
  "scripts": {
    "prebuild": "tsc --build ../core-gatsby-config/tsconfig.build.json",
    "build": "cross-env DEBUG=gastby:* gatsby build",
    "clean": "gatsby clean",
    "refresh": "curl -X POST http://localhost:9001/__refresh",
    "serve": "gatsby serve",
    "prestart": "gatsby clean",
    "start": "cross-env ENABLE_GATSBY_REFRESH_ENDPOINT=true gatsby develop -p 9001 -H 0.0.0.0",
    "sync-figma": "core-cli sync-figma ",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@newrade/core-design-system-old": "^1",
    "@newrade/core-react-ui-old": "^1",
    "gatsby": "^2"
  },
  "devDependencies": {
    "@newrade/core-gatsby-config": "^1",
    "@newrade/core-webpack-config": "^1"
  }
}
