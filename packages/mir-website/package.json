{
  "name": "@newrade/mir-website",
  "version": "1.7.0",
  "description": "Gatsby website for newrade's main website",
  "author": {
    "email": "info@newrade.ca",
    "name": "Newrade",
    "url": "https://github.com/newrade/newrade"
  },
  "repository": "https://github.com/newrade/newrade/tree/master/packages/newrade-website",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn compile",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "compile": "tsc --project tsconfig.gatsby.json",
    "pre-debug": "yarn clean && yarn compile",
    "preload-fonts": "cross-env PORT=9001 gatsby-preload-fonts --no-sandbox",
    "refresh": "curl -X POST http://localhost:9001/__refresh",
    "serve": "gatsby serve",
    "prestart": "yarn clean && yarn compile",
    "start": "cross-env ENABLE_GATSBY_REFRESH_ENDPOINT=true gatsby develop -p 9001 -H 0.0.0.0",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@newrade/core-design-system-old": "^1.3.3",
    "@newrade/core-react-ui-old": "^1.5.1",
    "gatsby": "^2.24"
  },
  "devDependencies": {
    "@newrade/core-gatsby-config": "^1.4.1",
    "@newrade/core-webpack-config": "^1.3.1"
  }
}
