{
  "name": "@newrade/core-github-actions",
  "version": "1.0.9",
  "description": "Custom Github actions",
  "author": {
    "email": "info@newrade.ca",
    "name": "Newrade",
    "url": "https://github.com/newrade/newrade"
  },
  "repository": "https://github.com/newrade/newrade-core/tree/master/packages/core-github-actions",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "prebuild-disabled": "yarn create-action-1 && yarn create-action-2",
    "build-disabled": "tsc --build tsconfig.build.json",
    "clean": "rimraf -r lib/** && rm -f tsconfig.build.tsbuildinfo",
    "create-action-1": "mkdir -p lib/remove-tsbuild-cache && cp 'src/remove-tsbuild-cache/action.yml' 'lib/remove-tsbuild-cache/action.yml'",
    "create-action-2": "mkdir -p lib/set-app-env && cp 'src/set-app-env/action.yml' 'lib/set-app-env/action.yml'",
    "start": "tsc --build --watch --preserveWatchOutput tsconfig.build.json"
  },
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@newrade/core-common": "^1.2.3",
    "@newrade/core-types": "^1.4.0",
    "@newrade/core-utils": "^1.6.3"
  }
}
