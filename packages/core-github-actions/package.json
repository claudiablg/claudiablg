{
  "name": "@newrade/core-github-actions",
  "version": "1.0.9",
  "description": "Custom Github actions",
  "author": {
    "email": "info@newrade.ca",
    "name": "Newrade",
    "url": "https://github.com/newrade/newrade"
  },
  "repository": "https://github.com/newrade/newrade-core/tree/master/packages/core-github-actions",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build": "yarn webpack",
    "clean": "rimraf -r lib/** && rm -f tsconfig.build.tsbuildinfo",
    "start": "tsc --build --watch --preserveWatchOutput tsconfig.build.json",
    "webpack": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json node -r ts-node/register ../../node_modules/webpack/bin/webpack --config webpack.prod.config.ts"
  },
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@newrade/core-common": "^1.2.3",
    "@newrade/core-types": "^1.4.0",
    "@newrade/core-utils": "^1.6.3"
  }
}
