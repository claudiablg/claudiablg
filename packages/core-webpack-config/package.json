{
  "name": "core-webpack-config",
  "version": "1.1.0",
  "description": "Reusable webpack configuration files",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "start": "tsc --build tsconfig.json --watch --preserveWatchOutput",
    "build": "tsc --build tsconfig.json",
    "prebuild": "yarn clean",
    "webpack": "webpack --config webpack.dev.config.ts",
    "test": "webpack --config webpack.test.config.ts",
    "clean": "rimraf -r lib/**"
  }
}
