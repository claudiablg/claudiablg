{
  "name": "@newrade/core-webpack-config",
  "version": "1.2.3",
  "description": "Reusable webpack configuration files",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf -r lib/**",
    "postinstall": "yarn build",
    "start": "tsc --build tsconfig.build.json --watch --preserveWatchOutput",
    "test": "TS_NODE_PROJECT=tsconfig.webpack.json node -r ts-node/register ../../node_modules/webpack/bin/webpack.js --config webpack.test.config.ts"
  },
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@newrade/core-jest-config": "^1"
  }
}
