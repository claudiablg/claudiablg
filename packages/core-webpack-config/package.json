{
  "name": "@newrade/core-webpack-config",
  "version": "2.0.0",
  "description": "Reusable webpack configuration files",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf lib/** && rm -f *.tsbuildinfo",
    "start": "tsc --build --watch --preserveWatchOutput --verbose",
    "typecheck": "tsc --noEmit"
  },
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@newrade/core-common": "^1",
    "@newrade/core-jest-config": "^2",
    "@newrade/core-types": "^2",
    "@newrade/core-utils": "^2"
  }
}
