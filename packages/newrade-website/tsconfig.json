{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "declaration": true,
    "sourceMap": true,
    "outDir": "./public/",
    "rootDir": "./",
    "strict": false,
    "target": "es6",
    "module": "esnext",
    "types": ["node"],
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "noEmit": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "typeRoots": ["../../node_modules/@types", "./src/typings"],
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
