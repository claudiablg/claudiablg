{
  "name": "@newrade/ze-design-system",
  "version": "1.3.0",
  "description": "Design system configuration package",
  "license": "MIT",
  "main": "dist/newrade-ze-design-system.cjs.js",
  "module": "dist/newrade-ze-design-system.esm.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:dist",
    "build:dist": "tsc --build tsconfig.json && cp src/*.cjs.d.ts src/*.cjs.js dist",
    "clean": "yarn clean:tsc && yarn clean:dist",
    "clean:dist": "rm -rf dist/** lib/**",
    "clean:tsc": "rm -f ./**/*.tsbuildinfo *.tsbuildinfo",
    "start": "yarn clean && yarn preconstruct dev"
  },
  "sideEffects": [
    "*.css",
    "*.treat.ts",
    "*.treat.js"
  ],
  "dependencies": {
    "@newrade/core-common": "^1",
    "@newrade/core-design-system": "^1",
    "@newrade/core-react-ui": "^2",
    "@newrade/core-types": "^2",
    "@newrade/core-utils": "^2"
  }
}
