{
  "name": "@newrade/core-utils",
  "version": "2.0.0",
  "description": "@newrade/core utilities shared across packages.",
  "author": {
    "email": "info@newrade.ca",
    "name": "Newrade",
    "url": "https://github.com/newrade/newrade"
  },
  "repository": {
    "directory": "packages/core-utils",
    "type": "git",
