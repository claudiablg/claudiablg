{
  "name": "@newrade/core-gatsby-starter",
  "version": "1.3.1",
  "description": "Gatsby website for newrade's main website",
  "author": {
    "email": "info@newrade.ca",
    "name": "Newrade",
    "url": "https://github.com/newrade/newrade"
  },
  "repository": "https://github.com/newrade/newrade/tree/master/packages/newrade-website",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn compile",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "compile": "tsc --project tsconfig.gatsby.json",
    "preload-fonts": "cross-env PORT=9003 gatsby-preload-fonts --no-sandbox",
    "refresh": "curl -X POST http://localhost:9003/__refresh",
    "serve": "gatsby serve",
    "prestart": "yarn compile",
    "start": "cross-env ENABLE_GATSBY_REFRESH_ENDPOINT=true gatsby develop -p 9003 -H 0.0.0.0",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@newrade/core-design-system": "^1",
    "@newrade/core-react-ui": "^1",
    "gatsby": "^2"
  },
  "devDependencies": {
    "@newrade/core-gatsby-config": "^1",
    "@newrade/core-utils": "^1",
    "@newrade/core-webpack-config": "^1"
  }
}
