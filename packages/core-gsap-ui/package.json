{
  "name": "@newrade/core-gsap-ui",
  "version": "1.3.4",
  "description": "UI library",
  "license": "MIT",
  "main": "dist/newrade-core-gsap-ui.cjs.js",
  "module": "dist/newrade-core-gsap-ui.esm.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "yarn copy:plugins && yarn build:tsc",
    "build:tsc": "tsc --build",
    "clean": "rm -rf lib/** && rm -f *.tsbuildinfo",
    "copy:plugins": "TS_NODE_PROJECT=../../tsconfig.node-commonjs.json node -r ts-node/register scripts/copy-plugins.script.ts",
    "start": "concurrently \"yarn tsc:start\" \"yarn watch:plugins\"",
    "tsc:start": "tsc --build --watch --preserveWatchOutput --verbose",
    "typecheck": "tsc --noEmit",
    "watch:plugins": "onchange -i 'src/plugins/**/*' -- yarn copy:plugins"
  },
  "sideEffects": false,
  "dependencies": {
    "gsap": "^3.6.0"
  }
}
