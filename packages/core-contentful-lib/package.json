{
  "name": "@newrade/core-contentful-lib",
  "version": "1.3.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf -r lib/** && rm -f tsconfig.build.tsbuildinfo",
    "export-content": ". .env && contentful space export --space-id=$CONTENTFUL_SPACEID --management-token=$CONTENTFUL_MANAGEMENT_TOKEN --environment-id=$CONTENTFUL_ENV --content-file=contentful-export-space.json",
    "import-content": "contentful space import",
    "run-migration": "cross-env TS_NODE_PROJECT=tsconfig.build.json node -r ts-node/register src/run-migration.ts",
    "start": "tsc --build --watch --preserveWatchOutput tsconfig.build.json"
  },
  "dependencies": {
    "contentful-migration": "^2.0.4"
  },
  "devDependencies": {
    "@contentful-tools/cli": "^0.7.3",
    "@newrade/core-common": "^1.0.1",
    "@newrade/core-gatsby-config": "^1.5.0",
    "@newrade/core-types": "^1.0.0",
    "@newrade/core-utils": "^1.3.0",
    "@newrade/core-webpack-config": "^1.3.3"
  }
}
