  "license": "MIT",
  "main": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf -r lib/** && rm -f tsconfig.build.tsbuildinfo",
    "execute": "cross-env TS_NODE_PROJECT=tsconfig.build.json node -r ts-node/register src/migrations/index.ts",
    "reset": "cross-env TS_NODE_PROJECT=tsconfig.build.json node -r ts-node/register src/migrations/reset.ts"
  },
  "dependencies": {
    "contentful-migration": "^2.0.4"
  },
  "devDependencies": {
    "@newrade/core-gatsby-config": "^1",
    "@newrade/core-webpack-config": "^1"
  }
}
