{
  "name": "@newrade/core-yo-generator",
  "version": "1.1.0",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git@github.com:cdpq-leo/ai-ui.git"
  },
  "author": {
    "name": "Philippe Leger",
    "email": "philippe.leger@newrade.ca",
    "url": "https://github.com/newrade/newrade"
  },
  "description": "Yeoman generator to scafold libraries, apps, components and more.",
  "bin": {
    "core-yo-generator": "./lib/src/cli.js"
  },
  "scripts": {
    "postinstall": "yarn build",
    "start": "tsc --build tsconfig.json --watch",
    "generator": "yo ./lib/src/ -- --no-insight --no-update-notifier",
    "build": "tsc --build tsconfig.json",
    "clean": "rimraf -r lib/**",
    "yo": "npm-run-all yo:tsc yo:start",
    "pretest": "yarn build",
    "test": "mocha lib/test/generator.test.js"
  },
  "dependencies": {
    "case": "^1.6.2",
    "inquirer": "^7.0.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-generator": "^4.2.0",
    "yeoman-test": "^2.0.0"
  }
}
