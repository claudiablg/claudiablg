{
  "name": "core-yo-generator",
  "version": "1.3.1",
  "description": "Yeoman generator to scafold libraries, apps, components and more.",
  "author": {
    "email": "philippe.leger@newrade.ca",
    "name": "Philippe Leger",
    "url": "https://github.com/newrade/newrade"
  },
  "repository": "https://github.com/newrade/newrade-core/tree/master/packages/core-yo-generator",
  "license": "MIT",
  "bin": {
    "nr-core-yo-generator": "./lib/src/cli.js"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf -r lib/**",
    "postinstall": "yarn build",
    "start": "tsc --build --watch --preserveWatchOutput",
    "test": "mocha lib/test/generator.test.js",
    "yo": "yo ./lib/src/ -- --no-insight --no-update-notifier"
  },
  "dependencies": {
    "case": "^1.6.2",
    "inquirer": "^7.0.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-generator": "^4.2.0",
    "yeoman-test": "^2.0.0"
  }
}
