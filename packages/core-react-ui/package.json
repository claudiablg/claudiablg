{
  "name": "@newrade/core-react-ui",
  "version": "2.1.0",
  "description": "UI library",
  "license": "MIT",
  "module": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build-disabled": "concurrently \"yarn tsc:build\" \"yarn copy:assets\"",
    "clean": "rm -rf lib/** && rm -f tsconfig.build.tsbuildinfo",
    "copy:assets": "TS_NODE_PROJECT=../core-webpack-config/tsconfig.script.json node -r ts-node/register scripts/copy-assets.script.ts",
    "start-disabled": "concurrently \"yarn tsc:start\" \"yarn watch:assets\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc:build": "tsc --build tsconfig.build.json",
    "tsc:start": "tsc --build tsconfig.build.json --watch --preserveWatchOutput",
    "typecheck": "yarn typecheck:build --watch --preserveWatchOutput",
    "typecheck:build": "tsc --project tsconfig.build.json --noEmit",
    "watch:assets": "onchange -i 'src/assets/**/*' -i 'src/icon-kits/newrade-icons/**/*' -- yarn copy:assets"
  },
  "sideEffects": [
    "*.treat"
  ],
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@newrade/core-common": "^1",
    "@newrade/core-design-system": "^1",
    "@newrade/core-gsap-ui": "^1.3.4",
    "@newrade/core-jest-config": "^2",
    "@newrade/core-types": "^2.0.0"
  },
  "devDependencies": {
    "@newrade/core-cli": "^1.11.0"
  }
}
