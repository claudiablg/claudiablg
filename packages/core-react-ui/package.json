{
  "name": "@newrade/core-react-ui",
  "version": "2.1.0",
  "description": "UI library",
  "license": "MIT",
  "module": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build": "yarn tsc:build",
    "build:dll": "yarn webpack:dll",
    "clean": "rm -rf lib/** && rm -f *.tsbuildinfo",
    "start": "yarn tsc:start",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc:build": "tsc --build",
    "tsc:start": "tsc --build --watch --preserveWatchOutput --verbose",
    "typecheck": "tsc --noEmit",
    "webpack": "TS_NODE_PROJECT=../../tsconfig.node-commonjs.json node -r ts-node/register ../../node_modules/webpack/bin/webpack",
    "webpack:dev": "yarn webpack serve --config webpack.dev.config.ts",
    "webpack:dll": "yarn webpack --config webpack.dll.config.ts"
  },
  "sideEffects": [
    "*.treat.ts",
    "*.treat.js"
  ],
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@newrade/core-common": "^1",
    "@newrade/core-design-system": "^1",
    "@newrade/core-gsap-ui": "^1.3.4",
    "@newrade/core-jest-config": "^2",
    "@newrade/core-types": "^2"
  },
  "devDependencies": {
    "@newrade/core-cli": "^1.11.0"
  }
}
