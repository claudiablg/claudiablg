{
  "name": "@newrade/core-figma-extractor",
  "version": "1.4.2",
  "description": "Utility to extract colors, text styles and other tokens from figma",
  "license": "MIT",
  "main": "./lib/commonjs/src/index.js",
  "module": "./lib/src/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build": "yarn build:commonjs && yarn build:esm",
    "build:commonjs": "tsc --build tsconfig.commonjs.json",
    "build:esm": "tsc --build",
    "clean": "rm -rf lib/** && rm -f *.tsbuildinfo",
    "extract": ". ./.env && DEBUG=$DEBUG cross-env TS_NODE_PROJECT=tsconfig.bin.json node -r ts-node/register bin/run.ts",
    "start": "concurrently \"yarn start:commonjs\" \"yarn start:esm\"",
    "start:commonjs": "tsc --build tsconfig.commonjs.json --watch --preserveWatchOutput --verbose",
    "start:esm": "tsc --build --watch --preserveWatchOutput --verbose",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "sideEffects": false,
  "typings": "./lib/src/index.d.ts",
  "dependencies": {
    "@newrade/core-common": "^1",
    "@newrade/core-design-system": "^1",
    "@newrade/core-types": "^2",
    "@newrade/core-utils": "^2",
    "figma-js": "^1.11.0"
  }
}
