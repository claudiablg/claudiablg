{
  "name": "@newrade/core-figma-extractor",
  "version": "1.4.2",
  "description": "Utility to extract colors, text styles and other tokens from figma",
  "license": "MIT",
  "main": "dist/newrade-core-figma-extractor.cjs.js",
  "module": "dist/newrade-core-figma-extractor.esm.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:dist && nr prepare-exports",
    "build:dist": "tsc --project tsconfig.json",
    "clean": "yarn clean:tsc && yarn clean:dist",
    "clean:dist": "rm -rf dist/** lib/**",
    "clean:tsc": "rm -f ./**/*.tsbuildinfo *.tsbuildinfo",
    "extract": ". ./.env && DEBUG=$DEBUG TS_NODE_PROJECT=tsconfig.bin.json node -r ts-node/register bin/run.ts",
    "extract2": ". ./.env && DEBUG=$DEBUG TS_NODE_PROJECT=../../tsconfig.node-commonjs.json node -r ts-node/register bin/run.ts",
    "start": "yarn clean && yarn preconstruct dev",
    "test": "nr jest",
    "test:watch": "nr jest --watch"
  },
  "sideEffects": false,
  "dependencies": {
    "@newrade/core-common": "^1",
    "@newrade/core-design-system": "^1",
    "@newrade/core-jest-config": "*",
    "@newrade/core-types": "^2",
    "@newrade/core-utils": "^2",
    "figma-js": "^1.11.0"
  }
}
