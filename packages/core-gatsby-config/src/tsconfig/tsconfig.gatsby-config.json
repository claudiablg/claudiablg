  "compilerOptions": {
    "outDir": "./",
    "noEmit": false,
    "declaration": true,
    "sourceMap": true,
    "rootDir": ".",
    "strict": false,
    "target": "esnext",
    "module": "commonjs",
    "types": ["node"],
    "typeRoots": ["../../node_modules/@types", "src/typings"]
