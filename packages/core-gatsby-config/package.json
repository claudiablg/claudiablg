{
  "name": "@newrade-core/core-gatsby-config",
  "version": "1.3.1",
  "description": "Reusable gatsby configuration files",
  "license": "MIT",
  "main": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf -r lib/**",
