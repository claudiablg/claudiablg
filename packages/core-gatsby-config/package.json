{
  "name": "@newrade/core-gatsby-config",
  "version": "2.1.0",
  "description": "Reusable gatsby configuration files",
  "license": "MIT",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "prebuild": "yarn create-package-json",
    "build": "yarn build:commonjs && yarn build:esm",
    "build:commonjs": "tsc --build tsconfig.commonjs.json",
    "build:esm": "tsc --build",
    "clean": "rm -rf lib/** && rm -f *.tsbuildinfo",
    "start": "concurrently \"yarn start:commonjs\" \"yarn start:esm\"",
    "start:commonjs": "tsc --build tsconfig.commonjs.json tsconfig.commonjs.json --watch --preserveWatchOutput --verbose",
    "start:esm": "tsc --build --watch --preserveWatchOutput --verbose",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "sideEffects": false,
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@newrade/core-common": "^1",
    "@newrade/core-types": "^2",
    "@newrade/core-utils": "^2",
    "@newrade/core-webpack-config": "^2"
  }
}
