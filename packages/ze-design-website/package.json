{
  "name": "@newrade/ze-design-website",
  "version": "2.1.0",
  "description": "Gatsby website for the Ze Design System website",
  "author": {
    "email": "info@newrade.ca",
    "name": "Newrade",
    "url": "https://github.com/newrade/newrade"
  },
  "repository": {
    "directory": "packages/ze-design-website",
    "type": "git",
    "url": "https://github.com/newrade/newrade-core.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "nr gatsby build --verbose",
    "build:local": ". ./.env && DEBUG=$DEBUG nr gatsby build --verbose",
    "clean": "gatsby clean && rm -rf ./node_modules/.cache",
    "pre-debug": "yarn clean",
    "preload-fonts": ". ./.env && PORT=$APP_PORT gatsby-preload-fonts --no-sandbox",
    "refresh": ". ./.env && curl -X POST http://localhost:$APP_PORT/__refresh",
    "serve": ". ./.env && DEBUG=$DEBUG nr gatsby serve --port=$APP_PORT --host 0.0.0.0",
    "start": ". ./.env && DEBUG=$DEBUG nr gatsby develop --verbose -p $APP_PORT -H 0.0.0.0",
    "test": "echo ok",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {},
  "sideEffects": [
    "*.css",
    "*.treat.ts",
    "*.treat.js"
  ],
  "dependencies": {
    "@newrade/core-common": "^1",
    "@newrade/core-design-system": "^1",
    "@newrade/core-design-system-docs": "^2",
    "@newrade/core-gatsb-config": "^2",
    "@newrade/core-gatsby-ui": "^2",
    "@newrade/core-gsap-ui": "^1.3.4",
    "@newrade/core-react-ui": "^2",
    "@newrade/core-types": "^2",
    "@newrade/core-utils": "^2",
    "@newrade/core-webpack-config": "^2",
    "@newrade/core-website-api": "^1",
    "@newrade/ze-design-system": "^1.3.0",
    "gatsby": "^3.10.2",
    "gatsby-plugin-image": "^1.11.0-next.2"
  }
}
