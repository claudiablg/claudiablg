{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Gatsby dev",
      "type": "pwa-node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/node_modules/.bin/gatsby",
      "args": ["develop", "--verbose"],
      "env": {
        "NVM_NODE_VERSION": "v15",
        "MASTER_GIT_HOST": "github.com",
        "MASTER_REPO_ORG": "newrade",
        "MASTER_REPO_NAME": "newrade",
        "PROJECT_GIT_HOST": "github.com",
        "PROJECT_REPO_ORG": "newrade",
        "PROJECT_REPO_NAME": "newrade"
      },
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "runtimeArgs": ["--preserve-symlinks", "--lazy"],
      "preLaunchTask": "pre-debug",
      "autoAttachChildProcesses": true,
      "sourceMaps": true
    },
    {
      "name": "Gatsby build",
      "type": "pwa-node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/node_modules/.bin/gatsby",
      "args": ["build", "--no-uglify", "--verbose"],
      "env": {
        "NVM_NODE_VERSION": "v15",
        "MASTER_GIT_HOST": "github.com",
        "MASTER_REPO_ORG": "newrade",
        "MASTER_REPO_NAME": "newrade",
        "PROJECT_GIT_HOST": "github.com",
        "PROJECT_REPO_ORG": "newrade",
        "PROJECT_REPO_NAME": "newrade"
      },
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "runtimeArgs": ["--preserve-symlinks", "--lazy"],
      "preLaunchTask": "pre-debug",
      "autoAttachChildProcesses": true,
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest debug current file",
      "program": "${workspaceFolder}/../../node_modules/jest/bin/jest.js",
      "args": [
        "${relativeFile}",
        "--no-cache",
        "--config=${workspaceFolder}/config/jest/jest.config.js"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
