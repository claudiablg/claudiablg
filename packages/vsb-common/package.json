{
  "name": "@newrade/vsb-common",
  "version": "1.3.1",
  "description": "Common models and other types between vsb-website and vsb-api",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build": "yarn build:commonjs && yarn build:esm",
    "build:commonjs": "tsc --build tsconfig.build.json",
    "build:esm": "tsc --build tsconfig.build.esm.json",
    "clean": "rimraf -r lib/** && rm -f tsconfig.build.tsbuildinfo",
    "start": "concurrently \"yarn start:commonjs\" \"yarn start:esm\"",
    "start:commonjs": "tsc --build tsconfig.build.json --watch --preserveWatchOutput",
    "start:esm": "tsc --build tsconfig.build.esm.json --watch --preserveWatchOutput"
  },
  "sideEffects": false,
  "typings": "./lib/esm/index.d.ts",
  "dependencies": {
    "@newrade/core-common": "^1.2.1",
    "@newrade/core-types": "^1.3.1",
    "@newrade/core-utils": "^1.6.1"
  },
  "devDependencies": {
    "@newrade/core-cli": "^1.6.0"
  }
}
